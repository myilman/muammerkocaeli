#include <stdio.h>
#include <stdlib.h>
//Calculates volume of sphere for given r

int main()
{
    int r;
    float v;
    float pi=3.1419;
    printf("enter the radius as integer cm\n");
    scanf("%d", &r);
    v=(4*pi*r*r*r)/3;
     printf("radius is %d cm \n", r);
     printf("Volume is %.2f cm", v);
    return 0;
}
