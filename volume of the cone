#include <stdio.h>
#include <stdlib.h>
//Calculates volume of the cone for given h and r

int main()
{
    int h;
    int r;
    float pi =3.1419;
    float volume;
    printf("Enter Height\n");
    scanf("%d", &h);
    printf("Enter Radius\n");
    scanf("%d", &r);
    volume=(pi * r * r * h)/3;
    printf("For height %d cm and radius %d cm\n", h,r);
    printf("Volume of the cone is %.2f cm cube\n", volume);
    return 0;
}
