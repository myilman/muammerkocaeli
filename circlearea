#include <stdio.h>
#include <stdlib.h>
//calculates area of the circle with radius by user input.
int main()
{
    float r;
    float pi=3.141592;
    float area;
    printf("Calculate the Area of Circle\n");
    printf("Enter Radius\n");
    scanf("%f", &r);
    area=r*r*pi;
    printf("Area of Circle is= %f", area);
    return 0;
}
